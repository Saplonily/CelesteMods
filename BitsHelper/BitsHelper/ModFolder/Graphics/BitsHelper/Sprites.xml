<Sprites>
  
  <bubbleEmitter path="objects/BitsHelper/bubbleEmitter/" start="idle">
    <Loop id="idle" path="close" delay="0.1" frames="3"/>
    <Anim id="open" path="open" delay="0.1" frames="0-1" goto="close"/>
    <Anim id="close" path="close" delay="0.1" frames="0-2" goto="idle"/>
  </bubbleEmitter>

  <bubble path="objects/BitsHelper/bubble/" start="appear">
    <Anim id="appear" path="appear" delay="0.1" frames="0-2" goto="idle"/>
    <Loop id="idle" path="idle" delay="0.1" frames="0"/>
    <Anim id="pop" path="pop" delay="0.01" frames="0-4"/>
  </bubble>

  <facingToggleSwapBlockLight path="objects/swapblock/moon/" start="idle">
    <Justify x="0.5" y="0.5" />
    <Loop id="idle" path="midBlock" delay="0.08" frames="0-3" />
  </facingToggleSwapBlockLight>

  <facingToggleSwapBlockLightRed path="objects/swapblock/moon/" start="idle">
    <Justify x="0.5" y="0.5" />
    <Loop id="idle" path="midBlockRed" delay="0.08" frames="0-3" />
  </facingToggleSwapBlockLightRed>
  
</Sprites>